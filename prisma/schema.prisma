// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model orders {
  iid          Int     @unique @default(autoincrement())
  id           String?
  flightNumber String?
  destination  String?
  origin       String?
  crew         Json?
  bookingInfo  Json?
  orderNo      String  @unique
}
